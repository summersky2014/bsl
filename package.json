{
  "name": "bsl",
  "version": "0.0.55",
  "description": "基于React的框架和UI组件",
  "main": "index.js",
  "scripts": {
    "test": "tsc --project tsconfig.test.json&&jest",
    "test:tscW": "tsc --project tsconfig.test.json -w",
    "test:jestW": "jest --watch",
    "vender": "set NODE_ENV=development&&webpack --env development --mode development --progress --color --display-error-details --config ./task/dll.ts",
    "publish": "npm run vender && npm publish",
    "docs:dev": "tsc --project tsconfig.task.json && set NODE_ENV=development&&webpack --env development --mode development --watch --progress --color --display-error-details",
    "docs:pro": "tsc --project tsconfig.task.json && set NODE_ENV=production&&webpack --env production --mode production --progress --color --display-error-details",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/summersky2014/bsl.git"
  },
  "keywords": [
    "React",
    "framework",
    "component"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/summersky2014/bsl/issues"
  },
  "homepage": "https://github.com/summersky2014/bsl#readme",
  "jest": {
    "preset": "jest-puppeteer",
    "testMatch": [
      "**/__tests__/tests/**/*.spec.js"
    ],
    "moduleNameMapper": {
      ".+\\.(css|scss)$": "<rootDir>/style-mock.js"
    }
  },
  "browserslist": [
    "Firefox > 20",
    "ie > 9",
    "chrome > 39",
    "iOS >= 7",
    "Android >= 4"
  ],
  "dependencies": {
    "@better-scroll/core": "next",
    "@better-scroll/slide": "^2.0.0-beta.2",
    "@better-scroll/wheel": "^2.0.0-beta.2",
    "@types/classnames": "^2.2.7",
    "@types/node": "^10.12.21",
    "@types/react": "^16.8.1",
    "@types/react-dom": "^16.0.11",
    "@types/react-router": "^4.4.3",
    "@types/react-router-dom": "^4.3.1",
    "@types/webpack": "^4.4.24",
    "@typescript-eslint/eslint-plugin": "^2.3.2",
    "@typescript-eslint/parser": "^2.3.2",
    "@ungap/url-search-params": "^0.1.2",
    "aphrodite": "^2.4.0",
    "autoprefixer": "^9.4.7",
    "axios": "^0.19.0",
    "babel-eslint": "^10.0.3",
    "classnames": "^2.2.6",
    "clean-webpack-plugin": "^2.0.1",
    "core-js": "^2.4.0",
    "create-subscription": "^16.7.0",
    "css-loader": "^1.0.1",
    "eslint": "^6.5.1",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-jsx-control-statements": "^2.2.1",
    "eslint-plugin-react": "^7.15.0",
    "exif-js": "^2.3.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "immer": "^5.0.0",
    "intersection-observer": "^0.7.0",
    "markdown-loader": "^5.1.0",
    "node-sass": "^4.11.0",
    "postcss-loader": "^2.1.3",
    "raw-loader": "^3.1.0",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "sass-loader": "6.0.7",
    "sass-resources-loader": "^1.3.3",
    "source-map-loader": "^0.2.4",
    "svg-sprite-loader": "^4.1.3",
    "terser-webpack-plugin": "^1.2.1",
    "ts-loader": "^5.3.3",
    "typescript": "^3.7.2",
    "typings-for-css-modules-loader": "1.7.0",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.2.1"
  },
  "devDependencies": {
    "@types/jest": "^24.0.0",
    "@types/jsdom": "^12.2.1",
    "@types/puppeteer": "^1.12.1",
    "@types/qrcode.react": "^0.9.0",
    "@types/react-syntax-highlighter": "^11.0.1",
    "@types/react-test-renderer": "^16.8.0",
    "@types/showdown": "^1.9.3",
    "antd": "^3.13.2",
    "copy-webpack-plugin": "^5.0.2",
    "http-server": "^0.11.1",
    "jest": "^24.1.0",
    "jest-dom": "^3.1.1",
    "jest-puppeteer": "^3.9.0",
    "jsdom": "^13.2.0",
    "puppeteer": "^1.12.2",
    "qrcode.react": "^0.9.3",
    "react-syntax-highlighter": "^11.0.2",
    "react-test-renderer": "^16.8.1",
    "showdown": "^1.9.0"
  }
}
